{{baseSelector}} {
line-height: 1;
}

{{baseSelector}}:before {
font-family: {{fontName}} !important;
font-style: normal;
font-weight: normal !important;
vertical-align: top;
}

{{#each codepoints}}

  %{{../classPrefix}}{{@key}} {
  content: "\\{{this}}";
  }

  .{{../classPrefix}}{{@key}}:before {
  content: "\\{{this}}";
  }
{{/each}}
